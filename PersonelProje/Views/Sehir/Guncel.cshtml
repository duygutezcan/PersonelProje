@model PersonelProje.Data.Sehir
@{
    ViewData["Title"] = "Guncel";
}

<h1>Guncelleme İşlemi</h1>

<div class="row">
    <div class="col-md-5">
        <div>
            <form method="post">
                <div>
                    <label>Id</label>
                    <input class="form-control" readonly type="text" value="@Model.Id" name="Id" />
                </div>
                <div>
                    <label>Ad</label>
                    <input class="form-control" type="text" value="@Model.SehirAd" name="SehirAd" />
                </div> 
                <input class="btn btn-success" type="submit" value="Güncelle"/>
            </form>

        </div>
    </div>
</div>